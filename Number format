import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int size=sc.nextInt();
        for(int row=0;row<size;row++){
            for(int col=0;col<size;col++){
                System.out.print(row+1);
            }
                System.out.println();
            }
            }
}
